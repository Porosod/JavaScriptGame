skeletons: []
types:
  - name: CharPlaceHold
    depth: 0
    oncreate: >+
      //this.speed = 5;

      goToXCo = this.x;//stops player from continuing forward after a room
      change

      playerXCo = this.x;

      movingArea = false;

      this.y= ct.room.playerStartY;

      if(entryDirection == 'left')

      {
          this.x = 64;
          ct.transition.slideIn(200,'right');
      }

      else if(entryDirection == 'right')

      {
          this.x = 768;
          ct.transition.slideIn(200,'left');
      }

      else if(entryDirection=='map')

      {
          console.log(entryDirection);
          this.x = 768;
          ct.transition.scaleIn(500);
      }

      else

      {
          this.x = 128;
      }

    onstep: >-
      this.move();

      if(ct.actions.Interact.pressed == true && dialogueMode == false &&
      questOpen == false && mapOpen == false)

      {
          if(ct.mouse.x >768)
          {   
              if(ct.room.nextRoomRight !='none'){
              ct.transition.slideOut(200,'right')
              .then(() => {
                  ct.rooms.switch(ct.room.nextRoomRight);
                  entryDirection = 'left';
              });
              }
          } 
          else if(ct.mouse.x <64)
          {
              if(ct.room.nextRoomLeft !='none'){
              ct.transition.slideOut(200,'left')
              .then(() => {
                  ct.rooms.switch(ct.room.nextRoomLeft);
                  entryDirection = 'right';
              });
              }
          }
      }

      moveActive = true;

      if(ct.actions.OpenQuestJournal.pressed)

      {
          if(questOpen == false)
          {
              exitQuest = false;
              this.questScreen = ct.rooms.append('QuestJournal', {isUi: true});
          }
      }

      if(ct.actions.OpenMap.pressed)

      {
          if(mapOpen == false)
          {
              exitMap = false;
              this.mapScreen = ct.rooms.append('Map', {isUi: true});
          }
      }

      if(ct.actions.Inventory.pressed)//if the I key is pressed

      {
          if(mapOpen == false)
          {
              exitQuest = false;
              this.inventoryScreen = ct.rooms.append('InventoryUIRoom', {isUi: true});
          }
      }
