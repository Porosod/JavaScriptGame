          if(ct.mouse.hovers(this) && ct.actions.Interact.pressed)
          {
              this.animationSpeed = 0.2;
              this.play();
              ct.transition.scaleOut(500)
              .then(() => {
                      ct.rooms.switch('R3');
                      entryDirection = 'map';
                  });
              exitMap = true;
              mapOpen = false;
          
