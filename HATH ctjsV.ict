  - name: Dialogue
    oncreate: >
      //384 320

      this.roomCharacter = ct.types.copy(currentCharacter + 'Dialogue',384,320);

      this.backgroundType = ct.types.copy('dialogueBack', 0, 0);

      exitDialogue = false;

      moveActive = false;

      console.log(currentCharacter);

      characterDialogueSetup();

      this.dialogue = newDialogue;

      this.dialogueText = new PIXI.Text(' ' + this.dialogue,
      ct.styles.get('Dialogue'));

      this.dialogueText.x = 96;//coordinates

      this.dialogueText.y = 480;

      this.dialogueText.depth = 1000;//depth is like the layer for example 1000
      would be a high up layer while -5 would be a low layer

      this.addChild(this.dialogueText);

      //response1 texts

      this.response1 = responseID1;

      this.response1Text = new PIXI.Text(this.response1,
      ct.styles.get('Response'));

      this.response1Text.x = 96;//coordinates

      this.response1Text.y = 528;

      this.response1Text.depth = 1000;//depth is like the layer for example 1000
      would be a high up layer while -5 would be a low layer

      this.addChild(this.response1Text);

      //response2 texts

      this.response2 = responseID2;

      this.response2Text = new PIXI.Text(this.response2,
      ct.styles.get('Response'));

      this.response2Text.x = 96;//coordinates

      this.response2Text.y = 560;

      this.response2Text.depth = 1000;//depth is like the layer for example 1000
      would be a high up layer while -5 would be a low layer

      this.addChild(this.response2Text);

      this.responseNumberCheck
    onstep: |-
      if(exitDialogue == true){
          ct.rooms.remove(this);
          this.roomCharacter.kill = true;
          this.backgroundType.kill = true;
          exitDialogue =false;
      }
      if(nextDialogue == true){
          nextDialogue = false;
          this.dialogue = newDialogue;
          console.log(responseID2);
          console.log(responseID1);
          this.response1 = responseID1;
          this.response2 = responseID2;
          //console.log(this.response2);
          //console.log(this.response1);
          //console.log(this.dialogue);
      }
      if(ct.actions.Interact.pressed){
          moveActive = false;
      }
    ondraw: |
      this.dialogueText.text =  this.dialogue;
      this.response1Text.text = this.response1;
      this.response2Text.text = this.response2;
    onleave: |-
      test = 2;
      moveActive = false;
