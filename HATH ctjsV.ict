      characterBlinking == false ){
          this.tex = characterBlink();
          this.gotoAndPlay(0);   
          characterBlinking = true;
          this.scale.x = 1;
      }

      if (this.currentFrame == 59){
          this.gotoAndStop(0);
          characterBlinking = false;
      }

      frameCounter++;

      if(frameCounter == 200){
          frameCounter = 0;
          sinceLastBlink++;
      }

      else if(frameCounter == 1 && sinceLastBlink > 5){

