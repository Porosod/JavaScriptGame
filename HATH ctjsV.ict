      /* Script for inventorys */
      var addedItemOrQuest;
      let inventoryArray = [];
      var runInventory = function(inputLine)
      {
          var responseLine;
          try//prevent game crash
          {
              if(inputLine.includes('001'))
              {
                  inventoryArray.push('Church Cloak\n');
                  responseLine = 'Here take this cloak as a symbol';
                  addedItemOrQuest = 'Church Cloak'
              }
              else if(inputLine.includes('002'))
              {
                  responseLine = 'sure';
                  inventoryArray.push('Church Handgun\n');
                  addedItemOrQuest = 'Church Handgun'
              }
          }
          catch
          {
              console.log('ERRORSTATE')
          }
          inventoryFormatter();
          return responseLine;
      }
      var inventoryFormatter = function()
      {//format inventory array
          var counter = inventoryArray.length
          while(counter!=0)
          {
              inventoryArray[counter-1] = (counter) + '  ' + inventoryArray[counter-1];
              counter--;
          }
      }
